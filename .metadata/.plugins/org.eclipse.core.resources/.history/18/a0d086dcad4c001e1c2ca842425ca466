package pessoa;

public class Pessoa {
	 String nome, cpf;
	 int idade, peso;
	
	public void Pessoa(String cpf, String nome, int peso, int idade) {
		this.nome = nome;
		this.idade = idade;
		this.peso = peso;
		this.cpf = cpf;
	}
	
	public void falar() {}
	public void dormir() {}
	public void ouvir() {}
	public void acordar() {}
}
